    "chksum": "https://deno.land/x/checksum@1.4.0/mod.ts",
    "imagemagick": "https://deno.land/x/imagemagick_deno@0.0.19/mod.ts",
    "zod": "https://deno.land/x/zod@v3.21.4/mod.ts",
    "date-fns": "npm:date-fns@2.29.3",
